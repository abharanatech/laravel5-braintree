    },
    "authors": [
        {
            "name": "Tameem Safi",
            "email": "tameemsafi@live.co.uk",
            "homepage": "http://oureastudios.com",
            "role": "Developer"
        },
        {
            "name": "Brad Daily",
            "email": "brad@bradleyboy.com",
            "role": "Developer"
        }
    ],
    "autoload": {
