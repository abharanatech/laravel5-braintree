{
    "name": "abharanatech/laravel5-braintree",
    "description": "Braintree Service Provider for Laravel 5",
    "keywords": ["braintree","braintree laravel 5"],
    "homepage": "https://github.com/abharanatech/laravel-braintree",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/abharanatech/laravel-braintree"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "5.x",
        "braintree/braintree_php": "3.35.0",
        "abharanatech/laravel-braintree": "dev-bugfix"
    },
    "authors": [
        {
            "name": "Abha Rana",
            "email": "abharana.techindustan@gmail.com",
            "homepage": "https://github.com/abharanatech",
            "role": "Developer"
        }
    ],
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Abharanatech\\Laravel": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
   
}
